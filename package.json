{
  "name": "careertrek-hacktoberfest-day-2018",
  "description": "Careertrek Hacktoberfest Day 2018",
  "version": "1.0.0",
  "author": "Chinu Cho <chinu.cho@bizreach.co.jp>",
  "dependencies": {
    "@types/react-helmet": "^5.0.7",
    "axios": "^0.18.0",
    "gatsby": "^2.0.27",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-plugin-typography": "^2.2.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typescript": "^3.1.3",
    "typography": "^0.16.17",
    "typography-theme-github": "^0.15.10"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --host 0.0.0.0",
    "serve": "gatsby serve",
    "format": "prettier --write 'src/*.js'",
    "test": "cucumber-js ./specs/*.feature --require-module ts-node/register --require './specs/*.ts'",
    "ci:dev": "cross-env NODE_ENV=development start-server-and-test develop http://0.0.0.0:8000 test",
    "ci:prod": "cross-env NODE_ENV=production gatsby build && start-server-and-test serve http://localhost:9000 test",
    "deploy": "gatsby build && gh-pages -d public -b gh-pages"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/cucumber": "^4.0.4",
    "@types/node": "^10.12.0",
    "@types/puppeteer": "^1.9.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "cucumber": "^5.0.2",
    "cucumber-html-reporter": "^4.0.4",
    "prettier": "^1.14.2",
    "puppeteer": "^1.9.0",
    "start-server-and-test": "^1.7.4",
    "ts-node": "^7.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/careertrek-hacktoberfest-day-2018/careertrek-hacktoberfest-day-2018.github.io"
  }
}
